{
	"author": "Eigil Nikolajsen",
	"dependencies": {
		"@tailwindcss/vite": "^4.1.17",
		"@types/bun": "^1.3.3",
		"mediabunny": "^1.25.2",
		"oxlint": "^1.30.0",
		"oxlint-tsgolint": "^0.8.2",
		"p5": "^2.1.1",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"solid-js": "^1.9.2",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"vite": "^7.2.4",
		"vite-plugin-solid": "^2.10.1",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"name": "canvas-starter",
	"packageManager": "bun@1.3.3",
	"private": true,
	"scripts": {
		"build": "bunx --bun vite build",
		"dev": "bunx --bun vite dev",
		"format": "bunx prettier . --write --experimental-cli",
		"lint": "oxlint --type-aware --type-check",
		"preview": "bun run build && bunx --bun vite preview",
		"reinstall": "rm -f bun.lock && bun install",
		"update": "bun update --interactive --recursive --force"
	},
	"type": "module",
	"version": "1.0.0"
}
